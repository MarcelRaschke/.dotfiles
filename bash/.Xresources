! COLOR_TERM_DARK_GREEN     #60b48a
! COLOR_TERM_DARK_YELLOW    #dfaf8f
! COLOR_TERM_DARK_BLUE      #506070
! COLOR_TERM_DARK_MAGENTA   #dc8cc3
! COLOR_TERM_DARK_CYAN      #8cd0d3
! COLOR_TERM_LIGHT_GRAY     COLOR_FG
! 
! COLOR_TERM_DARK_GRAY      #709080
! COLOR_TERM_LIGHT_RED      #dca3a3
! COLOR_TERM_LIGHT_GREEN    #c3bf9f
! COLOR_TERM_LIGHT_YELLOW   #f0dfaf
! COLOR_TERM_LIGHT_BLUE     #94bff3
! COLOR_TERM_LIGHT_MAGENTA  #ec93d3
! COLOR_TERM_LIGHT_CYAN     #93e0e3
! COLOR_TERM_WHITE          #ffffff

*foreground:         #dcdccc
*background:         #3f3f3f
! Black
